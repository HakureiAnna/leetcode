   "source": [
    "class Solution:\n",
    "    def mirrorReflection(self, p, q):\n",
    "        while p % 2 == 0 and q % 2 == 0:\n",
    "            p //= 2\n",
    "            q //= 2\n",
    "        return 1 - p%2 + q%2\n",
    "solution = Solution()"
   ]
